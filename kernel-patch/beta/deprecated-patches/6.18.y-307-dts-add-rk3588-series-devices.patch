+			port0: port@0 {
+				reg = <0>;
+				label = "eth0p0";
+				phy-handle = <&mdio_phy0>;
+			};
+
+			port1: port@1 {
+				reg = <1>;
+				label = "eth0p1";
+				phy-handle = <&mdio_phy1>;
+			};
+
+			port2: port@2 {
+				reg = <2>;
+				label = "eth0p2";
+				phy-handle = <&mdio_phy2>;
+			};
+
+			port3: port@3 {
+				reg = <3>;
+				label = "eth0p3";
+				phy-handle = <&mdio_phy3>;
+			};
+
+			port6: port@6 {
+				reg = <6>;
+				label = "cpu";
+				ethernet = <&gmac1>;
+				phy-mode = "rgmii";
+				tx-internal-delay-ps = <2000>;
+				rx-internal-delay-ps = <2100>;
+
+				fixed-link {
+					speed = <1000>;
+					full-duplex;
+				};
+			};
+		};
+	};
+};
